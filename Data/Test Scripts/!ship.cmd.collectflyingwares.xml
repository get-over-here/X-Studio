<script>
<name>!ship.cmd.collectflyingwares.pl</name>
<version>1</version>
<engineversion>20</engineversion>
<description>Ship Standard COMMAND COLLECT FLYING WARES</description>
<arguments>
<argument index="1" name="sector" type="Var/Sector" desc="the sector where to collect" />
</arguments>
<sourcetext>
<line linenr="001" indent=""><var>[THIS]&#160;-&gt;</var><text>&#160;set&#160;command:&#160;</text><var>COMMAND_COLLECT_FLYINGWARES</var><text>&#160;&#160;target=</text><var>$sector</var><text>&#160;target2=</text><var>null</var><text>&#160;par1=</text><var>null</var><text>&#160;par2=</text><var>null</var></line>
<line linenr="002" indent=""></line>
<line linenr="003" interruptable="@" indent=""><var>=&#160;</var><var>[THIS]&#160;-&gt;</var><text>&#160;call&#160;script&#160;</text><call>!move.movetosector</call><text>&#160;:&#160;</text><text>&#160;sector=</text><var>$sector</var></line>
<line linenr="004" indent=""></line>
<line linenr="005" indent=""><comment><text>*&#160;</text><var>now&#160;i&#160;am&#160;in&#160;the&#160;sector,&#160;search&#160;for&#160;wares</var></comment></line>
<line linenr="006" indent=""><var>$flags&#160;=&#160;</var><var>[Find.Nearest]</var><text>&#160;</text><var>|</var><text>&#160;</text><var>[Find.Multiple]</var></line>
<line linenr="007" indent=""><var>while&#160;</var><var>1</var></line>
<line linenr="008" indent="&#160;"></line>
<line linenr="009" indent=""><comment><text>*&#160;</text><var>find&#160;the&#160;5&#160;nearest&#160;wares</var></comment></line>
<line linenr="010" indent="&#160;"><var>$warearray&#160;=&#160;</var><text>&#160;find&#160;flying&#160;ware:&#160;sector=</text><var>[SECTOR]</var><text>&#160;maintype=</text><var>null</var><text>&#160;subtype=</text><var>null</var><text>&#160;flags=</text><var>$flags</var><text>&#160;refobj=</text><var>[THIS]</var><text>&#160;maxdist=</text><var>null</var><text>&#160;maxnum=</text><var>5</var><text>,&#160;refpos=</text><var>null</var></line>
<line linenr="011" indent="&#160;"></line>
<line linenr="012" indent="&#160;"><var>if&#160;</var><var>$warearray</var></line>
<line linenr="013" indent=""><comment><text>*&#160;</text><var>check&#160;if&#160;one&#160;of&#160;these&#160;wares&#160;fits&#160;into&#160;my&#160;ship</var></comment></line>
<line linenr="014" indent="&#160;&#160;"><var>$numwares&#160;=&#160;</var><text>&#160;size&#160;of&#160;array&#160;</text><var>$warearray</var></line>
<line linenr="015" indent="&#160;&#160;"></line>
<line linenr="016" indent="&#160;&#160;"><var>$i&#160;=&#160;</var><var>0</var></line>
<line linenr="017" indent="&#160;&#160;"><var>while&#160;</var><var>$i</var><text>&#160;</text><var>&lt;</var><text>&#160;</text><var>$numwares</var></line>
<line linenr="018" indent="&#160;&#160;&#160;"><var>$wareobj&#160;=&#160;</var><var>$warearray</var><text>[</text><var>$i</var><text>]</text></line>
<line linenr="019" indent="&#160;&#160;&#160;"><var>$ware&#160;=&#160;</var><var>$wareobj&#160;-&gt;</var><text>&#160;get&#160;ware&#160;type&#160;code&#160;of&#160;object</text></line>
<line linenr="020" indent="&#160;&#160;&#160;"><var>if&#160;</var><var>[THIS]&#160;-&gt;</var><text>&#160;can&#160;transport&#160;ware&#160;</text><var>$ware</var></line>
<line linenr="021" indent="&#160;&#160;&#160;&#160;"><var>if&#160;</var><var>[THIS]&#160;-&gt;</var><text>&#160;get&#160;free&#160;amount&#160;of&#160;ware&#160;</text><var>$ware</var><text>&#160;in&#160;cargo&#160;bay</text></line>
<line linenr="022" interruptable="@" indent="&#160;&#160;&#160;&#160;&#160;"><var>=&#160;</var><var>[THIS]&#160;-&gt;</var><text>&#160;call&#160;script&#160;</text><call>!move.collectware</call><text>&#160;:&#160;</text><text>&#160;GALOBJ&#160;of&#160;flying&#160;ware=</text><var>$wareobj</var><text>&#160;</text><text>&#160;destroy&#160;only=</text><var>[FALSE]</var></line>
<line linenr="023" indent="&#160;&#160;&#160;&#160;&#160;"><text>break</text></line>
<line linenr="024" indent="&#160;&#160;&#160;&#160;"><text>end</text></line>
<line linenr="025" indent="&#160;&#160;&#160;"><text>end</text></line>
<line linenr="026" indent="&#160;&#160;&#160;"><var>$i&#160;=&#160;</var><var>$i</var><text>&#160;</text><var>+</var><text>&#160;</text><var>1</var></line>
<line linenr="027" indent="&#160;&#160;"><text>end</text></line>
<line linenr="028" indent="&#160;"><text>end</text></line>
<line linenr="029" indent="&#160;"></line>
<line linenr="030" indent="&#160;"><var>$wait</var><text>&#160;=&#160;random&#160;value&#160;from&#160;</text><var>20000</var><text>&#160;to&#160;</text><var>30000</var><text>&#160;-&#160;1</text></line>
<line linenr="031" interruptable="@" indent="&#160;"><var>=&#160;</var><var>[THIS]&#160;-&gt;</var><text>&#160;move&#160;around&#160;</text><var>$wait</var><text>&#160;ms</text></line>
<line linenr="032" interruptable="@" indent="&#160;"><var>=&#160;</var><text>wait&#160;randomly&#160;from&#160;</text><var>100</var><text>&#160;to&#160;</text><var>200</var><text>&#160;ms</text></line>
<line linenr="033" indent=""><text>end</text></line>
<line linenr="034" indent=""><text>return&#160;</text><var>null</var></line>
</sourcetext>
